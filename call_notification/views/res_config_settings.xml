<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_call_notification_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.notification</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="60" />
        <field name="inherit_id" ref="base.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('settings')]" position="inside">
                <div class="app_settings_block" data-string="Notification Token" string="Call Notification" data-key="call_notification" >
                    <h2>Call Notification</h2>
                    <div class="row mt16 o_settings_container" name="managa_mail_campaigns_setting_container">
                        <div class="col-lg-12 o_setting_box col-12">
                            <div class="o_setting_right_pane">
                                <label for="call_token" />
                                <div>
                                    <field name="call_token"/>
                                </div>
                                <label for="register_api_endpoint" />
                                <div>
                                    <field name="register_api_endpoint"/>
                                </div>
                                <label for="finish_api_endpoint" />
                                <div>
                                    <field name="finish_api_endpoint"/>
                                </div>
                                <label for="show_api_endpoint" />
                                <div>
                                    <field name="show_api_endpoint"/>
                                </div>
                                <label for="hide_api_endpoint" />
                                <div>
                                    <field name="hide_api_endpoint"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>

</odoo>
